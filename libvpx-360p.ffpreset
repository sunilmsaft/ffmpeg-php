# convert to WebM

vcodec=libvpx

g=120
lag-in-frames=16
deadline=good
cpu-used=0
vprofile=0
qmax=63
qmin=0
b=768k

#ignored unless using -pass 2
maxrate=1.5M
minrate=40k
auto-alt-ref=1
arnr-maxframes=7
arnr-strength=5
arnr-type=centered


# this is a special directive that sets the maximum height to 360
#@height=360

# this directive sets the default extension
#@extension=webm

# this directive allows you to set the default output directory
## @directory=/path/to/output